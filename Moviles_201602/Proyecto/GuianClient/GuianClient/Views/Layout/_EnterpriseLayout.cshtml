@{
    GuianClient.ServiceEnterprise.enterprise objEnterprise = Session["objEnterprise"] == null ? null :
     ((GuianClient.ServiceEnterprise.enterprise)Session["objEnterprise"]);
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Guian UPC - @ViewBag.Title</title>
    <link rel="icon" href="@Url.Content("~/Content/images/logo-upc-blanco.png")">
    <!-- Bootstrap -->
    <link href="@Url.Content("~/Content/vendors/bootstrap/dist/css/bootstrap.min.css")" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="@Url.Content("~/Content/vendors/font-awesome/css/font-awesome.min.css")" rel="stylesheet">
    <!-- jQuery custom content scroller -->
    <link href="@Url.Content("~/Content/vendors/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.min.css")" rel="stylesheet">
    <!-- Custom Theme Style -->
    <link href="@Url.Content("~/Content/build/css/custom.min.css")" rel="stylesheet">
</head>
<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col menu_fixed">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <a href="index.html" class="site_title"> <img src="@Url.Content("~/Content/images/logo-upc-blanco.png")" width="30px" height="30px" alt="logo UPC"> <span>Guian UPC</span></a>
                    </div>
                    <div class="clearfix"></div>
                    <!-- menu profile quick info -->
                    <div class="profile">
                        <div class="profile_pic">
                            <img src="@Url.Content("~/Content/images/user.png")" alt="..." class="img-circle profile_img">
                        </div>
                        <div class="profile_info">
                            <span>Bienvenido,</span>
                            <h2>@ViewBag.Usuario</h2>
                            <h2>@objEnterprise.name</h2>
                        </div>
                    </div>
                    <!-- /menu profile quick info -->
                    <br />
                    <!-- sidebar menu -->
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                        <div class="menu_section">
                            <h3>General</h3>
                            <ul class="nav side-menu">
                                <li class="@(ViewBag.Section == "EntInicio" ? "active" : "")">
                                    <a href="@Url.Action("EntInicio", "Enterprise")"><i class="fa fa-home"></i> Inicio </a> <!--Una pagina de bienvenidos-->
                                </li>
                                <li class="@(ViewBag.Section == "LstTours" ? "active" : "")">
                                    <a href="@Url.Action("LstTours", "Enterprise")"><i class="fa fa-group"></i> Tours </a> <!--Una pagina de Agregar Tours-->
                                </li>
                                <li class="@(ViewBag.Section == "LstLugTours" ? "active" : "")">
                                    <a href="@Url.Action("LstLugTours", "Enterprise")"><i class="fa fa-institution"></i> Ver lugares de mis tours</a> <!--PRIMERO LISTAR LOS TOURS LUEGO DAR CLIC EN AGREGAR LUGAR Y SE LISTAN LOS LUGARES ||||Una pagina de Agregar Lugares a sus tours-->
                                </li>
                        </div>
                    </div>
                    <!-- /sidebar menu -->
                </div>
            </div>
            <!-- top navigation -->
            <div class="top_nav">
                <div class="nav_menu">
                    <nav class="" role="navigation">
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        </div>
                        <ul class="nav navbar-nav navbar-right">
                            <li class="">
                                <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                    <img src="@Url.Content("~/Content/images/user.png")" alt="">@ViewBag.Usuario
                                    <span class=" fa fa-angle-down"></span>
                                </a>
                                <ul class="dropdown-menu dropdown-usermenu pull-right">
                                 @* SI NOS PIDE OK  <li><a href="@Url.Action("EntProfile", "Enterprise")"> Perfil </a></li>*@
                                    <li><a href="@Url.Action("Logout", "Enterprise")"><i class="fa fa-sign-out pull-right"></i> Cerrar Sesión</a></li>
                                </ul>
                            </li>
                        </ul>

                    </nav>
                </div>
            </div>
            <!-- /top navigation -->
            <!-- page content -->
            <div class="right_col" role="main">
                <div class="page-title">
                    <div class="title_left">
                        <h3>@ViewBag.SubTitle</h3>
                    </div>
                </div>
                <div class="clearfix"></div>
                @RenderBody()
                <!-- /page content -->
                <!-- footer content -->
                <footer>
                    <div class="pull-right">
                        Guian UPC
                    </div>
                    <div class="clearfix"></div>
                </footer>
            </div>
            <!-- /footer content -->
        </div>
    </div>
    <!-- jQuery -->
    <script src="@Url.Content("~/Content/vendors/jquery/dist/jquery.min.js")"></script>
    <!-- Bootstrap -->
    <script src="@Url.Content("~/Content/vendors/bootstrap/dist/js/bootstrap.min.js")"></script>
    <!-- FastClick -->
    <script src="@Url.Content("~/Content/vendors/fastclick/lib/fastclick.js")"></script>
    <!-- NProgress -->
    <script src="@Url.Content("~/Content/vendors/nprogress/nprogress.js")"></script>
    <!-- jQuery custom content scroller -->
    <script src="@Url.Content("~/Content/vendors/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js")"></script>
    <!-- Custom Theme Scripts -->
    <script src="@Url.Content("~/Content/build/js/custom.min.js")"></script>
</body>
</html>