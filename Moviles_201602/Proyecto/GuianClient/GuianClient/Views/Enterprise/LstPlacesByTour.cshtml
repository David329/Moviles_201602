@model  GuianClient.ViewModel.EnterpriseVM.LstPlacesByTourVM
@{
    ViewBag.Title = "LstPlacesByTour";
    ViewBag.Section = "LstLugTours";
    Layout = "~/Views/Layout/_EnterpriseLayout.cshtml";
}

<div class="x_panel">
    <div class="x_title">
        <div class="title_left">
            <h2>Lista de Lugares</h2>
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="x_content">
        <table class="table table-striped table-hover">
            <thead>
                <tr>
                    <th>Audio</th>
                    <th>CityID</th>
                    <th>Description</th>
                    <th>Latitud</th>
                    <th>Longitud</th>
                    <th>Name</th>
                    <th>PlaceID</th>
                    <th>Visits</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.lstPlaces)
                {
                    <tr>
                        <td>@item.audio</td>
                        <td>@item.cityID</td>
                        <td>@item.description</td>
                        <td>@item.latitud</td>
                        <td>@item.longitud</td>
                        <td>@item.name</td>
                        <td>@item.placeID</td>
                        <td>@item.visits</td>
                    </tr>
                }
            </tbody>
        </table>
        <a class="btn btn-danger" href="@Url.Action("LstLugTours")"><i class="fa fa-long-arrow-left"></i> Regresar</a>
    </div>
</div>

